
<% users.each do |user|%>
  <% if user.display_status == true %>
    <div class="row">
      <div class="col-lg-2"><%= link_to (image_tag user.get_icon(100, 100)), public_user_path(user.id) %></div>
      <div class="col-lg-2">
        <%= user.name %><br>
        <%= CategoryMedium.find(user.category_medium_id).name %>
      </div>
    </div>
  <%end%>
<%end%>
