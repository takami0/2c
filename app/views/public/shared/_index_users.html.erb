<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user|%>
      <% if user.user_status == true %>
      <tr>
        <td><%= link_to (image_tag user.get_icon(100, 100)), public_user_path(user.id) %></td>
        <td><%= user.name %></td>
      </tr>
      <%end%>
    <%end%>
  </tbody>
</table>